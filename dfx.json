{
  "canisters": {
    "triip": {
      "main": "src/triip/main.mo",
      "type": "motoko",
      "dependencies": [
        "triip_rust"
      ]
    },
    "triip_rust": {
      "type": "custom",
      "candid": "src/triip_rust/src/triip_rust.did",
      "build": "sh ./build.sh",
      "wasm": "target/wasm32-unknown-unknown/release/triip_rust-opt.wasm"
    },
    "triip_token": {
      "main": "src/triip_token/token.mo",
      "type": "motoko"
    },
    "triip_assets": {
      "dependencies": [
        "triip"
      ],
      "frontend": {
        "entrypoint": "src/triip_assets/src/index.html"
      },
      "source": [
        "src/triip_assets/assets",
        "dist/triip_assets/"
      ],
      "type": "assets"
    },
    "triip_ar": {
      "frontend": {
        "entrypoint": "src/triip_ar/index.html"
      },
      "source": [
        "src/triip_ar"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": "vessel sources"
    }
  },
  "dfx": "0.8.4",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}